<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="100.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="QuestionWindow.QuestionWindowController">
   <children>
      <Label fx:id="message" layoutX="30.0" layoutY="10.0" text="Label" />
      <Button fx:id="actionRun" layoutX="100.0" layoutY="50.0" mnemonicParsing="false" onAction="#actionRun" text="No" />
      <Button fx:id="noBtn" layoutX="150.0" layoutY="50.0" mnemonicParsing="false" onAction="#closeButton" text="Cancel" />
      <Button fx:id="replace" layoutX="151.0" layoutY="81.0" mnemonicParsing="false" onAction="#replace" text="Replace" />
      <Button fx:id="replaceAll" layoutX="50.0" layoutY="50.0" mnemonicParsing="false" onAction="#replaceAll" text="Yes" />
   </children>
</AnchorPane>
